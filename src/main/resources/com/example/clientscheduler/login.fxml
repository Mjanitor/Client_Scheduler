<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clientscheduler.LoginController">
    <children>
        <AnchorPane fx:id="HelloController" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <AnchorPane layoutX="119.0" layoutY="140.0" prefHeight="101.0" prefWidth="371.0">
                    <children>
                        <TextField fx:id="username" focusTraversable="false" layoutX="7.0" layoutY="14.0" promptText="Username" />
                        <TextField fx:id="password" focusTraversable="false" layoutX="7.0" layoutY="49.0" promptText="Password" />
                        <Button fx:id="loginButton" focusTraversable="false" layoutX="229.0" layoutY="37.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
                  <Label fx:id="bad_pw" layoutX="3.0" layoutY="-8.0" text="Incorrect Username/Password" textFill="RED" visible="false" />
                    </children>
                </AnchorPane>
            <Label fx:id="location" layoutX="119.0" layoutY="53.0" text="Location" />
            </children>
        </AnchorPane>
    </children>
</VBox>
