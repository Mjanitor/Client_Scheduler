<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.clientscheduler.HelloController">
    <children>
        <AnchorPane fx:id="HelloController" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <AnchorPane layoutX="119.0" layoutY="140.0" prefHeight="101.0" prefWidth="371.0">
                    <children>
                        <TextField fx:id="username" layoutX="7.0" layoutY="14.0" promptText="Username" />
                        <TextField fx:id="password" layoutX="7.0" layoutY="49.0" promptText="Password" />
                        <Button fx:id="login" layoutX="229.0" layoutY="37.0" mnemonicParsing="false" onAction="#login" text="Login" />
                    </children>
                </AnchorPane>
            <Label fx:id="location" layoutX="119.0" layoutY="53.0" text="Location" />
            </children>
        </AnchorPane>
    </children>
</VBox>
